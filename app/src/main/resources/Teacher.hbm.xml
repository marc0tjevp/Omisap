<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="nl.scalda.pasimo.model.employeemanagement.Teacher" table="teacher">
      <meta attribute="class-description">
         This class contains the teacher detail. 
      </meta>
      <id name="employeeNumber" type="integer" column="employeeNumber">
      </id>
      <property name="abbreviation" column="abbreviation" type="string"/>
   </class>
</hibernate-mapping>